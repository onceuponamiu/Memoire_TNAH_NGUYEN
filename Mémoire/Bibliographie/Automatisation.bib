@incollection{bourgeoisUsingTopicGeneration2022,
  title = {Using {{Topic Generation Model}} to Explore the {{French Parliamentary Debates}} during the Early {{Third Republic}} (1881-1899)},
  booktitle = {{{DiPaDA}} 2022 {{Digital Parliamentary Data}} in {{Action}} 2022. {{Proceedings}} of the {{Digital Parliamentary Data}} in {{Action}} ({{DiPaDA}} 2022) {{Workshop}} Co-Located with 6th {{Digital Humanities}} in the {{Nordic}} and {{Baltic Countries Conference}} ({{DHNB}} 2022)},
  author = {Bourgeois, Nicolas and Pellet, Aurélien and Puren, Marie},
  editor = {Mela, Matti La and Norén, Fredrik and Hyvönen, Eero},
  date = {2022-05},
  series = {{{CEUR Workshop Proceedings}}},
  pages = {35--51},
  url = {https://hal.archives-ouvertes.fr/hal-03526254},
  urldate = {2022-08-09},
  abstract = {In this long paper, we use NLP techniques to explore two decades (1881-1899) of parliamentary debates of the French Third Republic (1870-1940), and more specifically to analyse the importance of the army in the political debate. We use Latent Dirichlet Allocation to partition the vocabulary into topics, and then study the distribution of the topic "army" over time. We also examine its connection with other topics, in relation to the main political and military events of the period.},
  keywords = {Early Third Republic (1881-1899),France,Natural Language Processing,Parliamentary Debates,Topic Modelling}
}

